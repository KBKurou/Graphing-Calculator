<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyHeight="5%p">

    <Row android:keyWidth="9%p">
        <Key android:codes="120" android:keyLabel="x"
            android:keyEdgeFlags="left"/>
        <Key android:codes="121" android:keyLabel="y" />
        <Key android:codes="60001" android:keyLabel="x²" />
        <Key android:codes="60002" android:keyLabel="xⁿ" />

        <Key android:codes="55" android:keyLabel="7"
            android:horizontalGap="5%p"/>
        <Key android:codes="56" android:keyLabel="8"  />
        <Key android:codes="57" android:keyLabel="9"  />
        <Key android:codes="47" android:keyLabel="÷"  />

        <Key android:codes="60003" android:keyLabel="Θ" android:horizontalGap="5%p"
            android:popupKeyboard="@xml/popup"
            android:keyWidth="18%p" android:keyEdgeFlags="right"/>
    </Row>

    <Row android:keyWidth="9%p">
        <Key android:codes="40" android:keyLabel="(" android:keyEdgeFlags="left"/>
        <Key android:codes="41" android:keyLabel=")"/>
        <Key android:codes="77001" android:keyLabel="ln" android:keyOutputText="ln( )"/>
        <Key android:codes="77002" android:keyLabel="log" android:keyOutputText="log( )"/>

        <Key android:codes="52" android:keyLabel="4" android:horizontalGap="5%p"/>
        <Key android:codes="53" android:keyLabel="5" />
        <Key android:codes="54" android:keyLabel="6" />
        <Key android:codes="42" android:keyLabel="×" />

        <Key android:codes="55002" android:keyLabel="←" android:horizontalGap="5%p"
            android:isRepeatable="true" />
        <Key android:codes="55003" android:keyLabel="→"
            android:isRepeatable="true"  android:keyEdgeFlags="right"/>
    </Row>

    <Row android:keyWidth="9%p">
        <Key android:codes="60004" android:keyLabel="√" android:keyEdgeFlags="left"
            android:keyOutputText="sqrt( "/>
        <Key android:codes="60005" android:keyLabel=","/>
        <Key android:codes="600020" android:keyLabel="e"/>
        <Key android:codes="600021" android:keyLabel="π"/>

        <Key android:codes="49" android:keyLabel="1" android:horizontalGap="5%p"/>
        <Key android:codes="50" android:keyLabel="2" />
        <Key android:codes="51" android:keyLabel="3" />
        <Key android:codes="45" android:keyLabel="-" />

        <Key android:codes="-5" android:keyLabel="⌫" android:horizontalGap="5%p"/>
        <Key android:codes="55006" android:keyLabel="CLR" android:keyEdgeFlags="right"/>
    </Row>

    <Row android:keyWidth="9%p">
        <Key android:codes="66000" android:keyLabel="sin" android:keyOutputText="sin( )"
            android:keyEdgeFlags="left" />
        <Key android:codes="66001" android:keyLabel="cos" android:keyOutputText="cos( )" />
        <Key android:codes="60008" android:keyLabel="tan" android:keyOutputText="tan( )" />
        <Key android:codes="60009" android:keyLabel="cot" android:keyOutputText="cot( )" />

        <Key android:codes="50" android:keyLabel="0"
            android:horizontalGap="5%p"/>
        <Key android:codes="46" android:keyLabel="."  />
        <Key android:codes="61" android:keyLabel="=" />
        <Key android:codes="43" android:keyLabel="+" />

        <Key android:codes="-3" android:keyLabel="hide kb"
            android:keyEdgeFlags="right" android:keyWidth="18%p"
            android:horizontalGap="5%p"/>
    </Row>

    <!--OLD-->

</Keyboard>
